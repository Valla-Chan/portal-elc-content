// envmaptint_fix
"LightmappedGeneric"
{

	"$basetexture" "nature/hazard_liquid_blue_beneath"
	"$bumpmap" "Nature/slime_normal"
	"$abovewater" 0

	"$selfillum" 1
	"$surfaceprop" "slime"
	"$envmap" "env_cubemap"
	"$envmaptint" "[ 1.00 .89 .80 ]"
	"$envmapcontrast" "1"

	"%keywords" "portal"
	"%compileSlime" 1

	"$fogenable" 1
	"$fogcolor" "{72 70 120}"
	"$fogstart" 1.00
	"$fogend" 100.00

	
	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$bumpmap"
			"animatedtextureframenumvar" "$bumpframe"
			"animatedtextureframerate" 21.00
		}
	}
}


